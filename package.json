  "homepage": "https://github.com/xSyki/page-dumper#readme",
  "scripts": {
    "start": "prisma generate --schema=./src/prisma/schema.prisma && next dev",
    "build": "prisma generate --schema=./src/prisma/schema.prisma && prisma db push --schema=./src/prisma/schema.prisma && next build",
    "serve": "next start",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "push-prisma": "prisma db push --schema=./src/prisma/schema.prisma",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "email-dev": "email dev",
    "email-export": "email export"
  },
